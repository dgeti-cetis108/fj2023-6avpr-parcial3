<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PaseDeListaV.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PaseDeListaV"
    xmlns:examples="clr-namespace:PaseDeListaV.Views.Examples"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Flex">
            <ShellContent
                ContentTemplate="{DataTemplate examples:Flex}"/>
        </Tab>
        <Tab Title="Main">
            <ShellContent
                ContentTemplate="{DataTemplate local:MainPage}"/>
        </Tab>
    </TabBar>

    <ShellContent
        ContentTemplate="{DataTemplate examples:Grid}"
        Route="GridLayout"/>



</Shell>

